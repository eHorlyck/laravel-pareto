{
    "name": "ehorlyck/laravel-pareto",
    "description": "Opinionated fast-tracking of laravel development",
    "keywords": [
        "eHorlyck",
        "laravel-pareto"
    ],
    "homepage": "https://github.com/ehorlyck/laravel-pareto",
    "license": "MIT",
    "authors": [
        {
            "name": "Emil HÃ¸rlyck",
            "email": "emilha@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^2.3",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "EHorlyck\\LaravelPareto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EHorlyck\\LaravelPareto\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
